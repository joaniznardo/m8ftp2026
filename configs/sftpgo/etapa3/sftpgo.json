    "disable_active_mode": false,
    "certificate_file": "/etc/sftpgo/certs/demoftp.test.crt",
    "certificate_key_file": "/etc/sftpgo/certs/demoftp.test.key"
  },
  "sftpd": {
    "bindings": [
      {
        "port": 0
      }
