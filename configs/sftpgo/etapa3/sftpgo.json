  "httpd": {
    "bindings": [
      {
        "port": 8080,
        "address": "",
        "enable_web_admin": true,
        "enable_web_client": true,
        "enable_rest_api": true,
        "enable_https": true,
        "min_tls_version": 12,
        "branding": {
          "name": "SFTPGo Lab - Etapa 3 (S3 Storage)",
          "short_name": "Lab3"
        },
