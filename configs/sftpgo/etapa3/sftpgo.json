    "certificate_key_file": "/etc/sftpgo/certs/demoftp.test.key",
    "hide_support_link": true
  },
  "data_provider": {
    "driver": "sqlite",
    "name": "/var/lib/sftpgo/sftpgo.db",
    "users_base_dir": "/srv/sftpgo/data",
